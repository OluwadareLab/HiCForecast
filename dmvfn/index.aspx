					while (c.charAt(0)==' ') c = c.substring(1, c.length); //delete spaces
					if (c.indexOf(nameEQ) == 0) {
					    //console.log("readCookie(name):" + name + "Value:" + c.substring(nameEQ.length, c.length));
					    return c.substring(nameEQ.length, c.length);
